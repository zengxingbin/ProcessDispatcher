            <Label fx:id="inputTip" prefHeight="15.0" prefWidth="141.0" text="  请输入初始进程个数：" />
            <TextField fx:id="initProNum" prefHeight="23.0" prefWidth="210.0" promptText="（1~10个）" GridPane.columnIndex="1" />
         </children>
      </GridPane>
      <Label fx:id="invalidInputTip" layoutX="145.0" layoutY="59.0" text="请输入1~10之间的数字！" textFill="#d30b0b" visible="false" />
